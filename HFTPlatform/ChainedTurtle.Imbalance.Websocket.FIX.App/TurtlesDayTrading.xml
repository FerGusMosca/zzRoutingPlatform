<?xml version="1.0" encoding="utf-8" ?>
<ChainedConfiguration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<Name>CHAINED_TURTLES_IMB_WEBSOCKET_FIX_MOCK_V1_1</Name>
	<Account>xxx</Account>

	<PositionSizeInCash>1400000</PositionSizeInCash>
	<MaxOpenedPositions>6</MaxOpenedPositions>
	<!--1=1%-->
	<FeeTypePerTrade>Nominal</FeeTypePerTrade>
	<!--OrderRouter,Nominal,Percentage-->
	<FeeValuePerTrade>0</FeeValuePerTrade>
	<DecimalRounding>3</DecimalRounding>

	<OpeningTime>10:30</OpeningTime>
	<!--Opening Time for backtests or LIVE_MODE-->
	<TradingMode>LIVE_MODE</TradingMode>
	<!--BACKTEST,LIVE_MODE-->

	<!--Needed for historical prices recovery-->
	<ClosingTime>05:51 PM</ClosingTime>

	<SkipTrendlinesDeletion>true</SkipTrendlinesDeletion>

	<OrderRouter>zHFT.OrderRouters.Router.OrderRouter, zHFT.OrderRouters</OrderRouter>

	<OrderRouterConfigFile>.\GenOrderRouter.xml</OrderRouterConfigFile>

	<ConnectionString>Data Source=.\SQLDEV;initial catalog=chained_turtles_dev;integrated security=True;multipleactiveresultsets=True</ConnectionString>
	<ResetOnPersistance>true</ResetOnPersistance>
	<CancelActiveOrdersOnStart>false</CancelActiveOrdersOnStart>
	<BulkMarketDataRequest>false</BulkMarketDataRequest>
	<!-- IMPORTANT: bacause candle CLOSE prices do not always have, the candle close -->

	<OnlyLong>false</OnlyLong>

	<SecuritiesToMonitor>
		<SecurityToMonitor>
			<Symbol>GGAL</Symbol>
			<SecurityType>CS</SecurityType>
			<!-- Default: CS-->
			<Currency>ARS</Currency>
			<!-- Default: USD-->
			<Exchange>BUE</Exchange>
			<MonitoringType>ONLY_ROUTING</MonitoringType>
			<!-- Default: SMART -->

			<Indicators>
