<?xml version="1.0" encoding="utf-8" ?>
<Configuration>
	<Name> Multiple Market Client</Name>


	<MarketCLients>
		<MarketClient> <!-- FIX Connector -->
			<IncomingConfigPath>.\Websocket\IncomingSingletonConfigFIX.xml</IncomingConfigPath>
			<IncomingModule>zHFT.SingletonModulesHandler.SingletonModulesHandler, zHFT.SingletonModulesHandler</IncomingModule>
			<Type>PUBLISH</Type><!--INCOMING, PUBLISH-->
			<PublisherKey>GenOrderRouterModule</PublisherKey>
		</MarketClient>



		<MarketClient><!-- IB Connector  -->
			<IncomingConfigPath>.\Websocket\IncomingSingletonConfigIB.xml</IncomingConfigPath>
			<IncomingModule>zHFT.SingletonModulesHandler.SingletonModulesHandler, zHFT.SingletonModulesHandler</IncomingModule>
			<Type>INCOMING</Type><!--INCOMING, PUBLISH-->
			<PublisherKey></PublisherKey>
		</MarketClient>
		
		
		
	</MarketCLients>


	<PublishClients>
		<PublishClient>
			<!--Singleton to Generic Order Router to publish FIX market data-->
			<Key>GenOrderRouterModule</Key>
			<OutgoingConfigPath></OutgoingConfigPath><!--TODO!: Singleton Gen Order Router-->
			<OutgoingModule>zHFT.OrderRouters.Router.SingletonOrderRouter, zHFT.OrderRouters</OutgoingModule><!--TODO!: Singleton Gen Order Router-->
		</PublishClient>
		
		
	</PublishClients>
	
	
</Configuration>