<?xml version="1.0" encoding="utf-8" ?>
<Configuration>
  <Name>Incoming IB</Name>
  <!--Para los archivos de logs-->
  <Active>true</Active>
  <IP>localhost</IP>
  <Port>7496</Port>
  <IdIBClient>1</IdIBClient>

  <StockListAccessLayer></StockListAccessLayer>
  <StockListAccessLayerConnectionString></StockListAccessLayerConnectionString>

  <PublishUpdateInMilliseconds>5000</PublishUpdateInMilliseconds><!--5 segundos-->

  <MarketDataDebugActive>true</MarketDataDebugActive>
  <MarketDataDebugRefresh>5000</MarketDataDebugRefresh>

  <IdPortfolio>0</IdPortfolio><!--Poner 0 si no aplica ningún portfolio-->

  <OnlyNotProcessed>true</OnlyNotProcessed>

  <Contract>
    <Symbol>HPQ</Symbol>
    <SecType>STK</SecType>
    <Currency>USD</Currency>
    <Exchange>SMART</Exchange>
  </Contract>

  

</Configuration>