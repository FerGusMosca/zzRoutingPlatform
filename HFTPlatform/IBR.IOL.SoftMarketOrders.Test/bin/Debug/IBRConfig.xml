<?xml version="1.0" encoding="utf-8" ?>
<Configuration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Name>Strategy Handler - Instruction Based Router - Soft Market Routing</Name>

  <ReportSavingMode>BD</ReportSavingMode>
  <!--BD=SQL Server Data Base, EXCEL=CSV file, NONE=nothing-->
  <ReportSavingConnectionString>metadata=res://*/StrategyReportsEntityDataModel.csdl|res://*/StrategyReportsEntityDataModel.ssdl|res://*/StrategyReportsEntityDataModel.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=.\SQLDEV;initial catalog=StrategyReports;integrated security=True;multipleactiveresultsets=True;App=EntityFramework&quot;</ReportSavingConnectionString>

  <OrderRouter>zHFT.OrderRouters.Router.SoftMarketOrderRouter, zHFT.OrderRouters</OrderRouter>

  <OrderRouterConfigFile>.\GenericOrderRouter.xml</OrderRouterConfigFile>

  <InstructionsAccessLayerConnectionString >metadata=res://*/AutPortfolioEntityDataModel.csdl|res://*/AutPortfolioEntityDataModel.ssdl|res://*/AutPortfolioEntityDataModel.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=.\SQLDEV;initial catalog=AutPortfolio;integrated security=True;multipleactiveresultsets=True;App=EntityFramework&quot;</InstructionsAccessLayerConnectionString >

  <AccountNumber>10</AccountNumber> <!--IOL PROD-->

  <RoutingUpdateInMilliseconds>1000</RoutingUpdateInMilliseconds>

  <AccoutReferenceHandler>zHFT.StrategyHandler.IBR.IOL.DataAccessLayer.IOLAccountManager, zHFT.StrategyHandler.IBR.IOL.DataAccessLayer</AccoutReferenceHandler>

  <AccountManagerAccessLayer>zHFT.StrategyHandler.InstructionBasedRouting.DataAccessLayer.Managers.AccountManager, zHFT.StrategyHandler.InstructionBasedRouting.DataAccessLayer</AccountManagerAccessLayer>

  <InstructionManagerAccessLayer>zHFT.StrategyHandler.InstructionBasedRouting.DataAccessLayer.Managers.InstructionManager,zHFT.StrategyHandler.InstructionBasedRouting.DataAccessLayer</InstructionManagerAccessLayer>

  <PositionManagerAccessLayer>zHFT.StrategyHandler.InstructionBasedRouting.DataAccessLayer.Managers.PositionManager,zHFT.StrategyHandler.InstructionBasedRouting.DataAccessLayer</PositionManagerAccessLayer>

  <AccountManagerConfig>
    <Config>
      <Key>AccountNumber</Key>
      <!--<Value>9</Value>--><!--IOL Test-->
      <Value>10</Value><!--IOL Prod-->
    </Config>
    <Config>
      <Key>ConfigConnectionString</Key>
      <Value>Data Source=.\SQLDEV;Initial Catalog=AutPortfolio;Integrated Security=SSPI;</Value>
    </Config>
    <Config>
      <Key>MainURL</Key>
      <!--<Value>https://api-sandbox.invertironline.com/</Value>--><!--Invertir Online Test-->
      <Value>https://api.invertironline.com/</Value> <!--Invertir Online PROD-->
    </Config>
  </AccountManagerConfig>

</Configuration>