<?xml version="1.0" encoding="utf-8" ?>
<ChainedConfiguration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<Name>CHAINED_TURTLES_RNN_LSTM_EXTERNAL_SIGN_WEBSOCKET_IB_MOCK</Name>
	<Account>xxx</Account>

	<PositionSizeInCash>50000</PositionSizeInCash>
	
	<MaxOpenedPositions>6</MaxOpenedPositions>
	
	<!--1=1%-->
	<FeeTypePerTrade>Nominal</FeeTypePerTrade>
	<!--OrderRouter,Nominal,Percentage-->
	<FeeValuePerTrade>0</FeeValuePerTrade>
	<DecimalRounding>3</DecimalRounding>

	<OpeningTime>11:30</OpeningTime><!--Opening Time for backtests or LIVE_MODE-->
	<!--Needed for historical prices recovery-->
	<ClosingTime>05:51 PM</ClosingTime>
	<TradingMode>LIVE_MODE</TradingMode><!--BACKTEST,LIVE_MODE-->

	<OrderRouter>zHFT.OrderRouters.Router.OrderRouter, zHFT.OrderRouters</OrderRouter>

	<OrderRouterConfigFile>.\GenOrderRouter.xml</OrderRouterConfigFile>

	<ConnectionString>Data Source=.\SQLDEV;initial catalog=chained_inv_vix_bob_dev;integrated security=True;multipleactiveresultsets=True</ConnectionString>
	<ResetOnPersistance>true</ResetOnPersistance>
	
	<CancelActiveOrdersOnStart>false</CancelActiveOrdersOnStart>
	<BulkMarketDataRequest>false</BulkMarketDataRequest>
	<!-- IMPORTANT: bacause candle CLOSE prices do not always have, the candle close -->
	<OnlyLong>false</OnlyLong>

	<SecuritiesToMonitor>
		<SecurityToMonitor>
			<Symbol>SPY</Symbol>
			<SecurityType>CS</SecurityType> <!-- Default: CS-->
			<Currency>USD</Currency> <!-- Default: USD-->
			<Exchange>SMART</Exchange><!-- Default: SMART -->
			<MonitoringType>ONLY_ROUTING</MonitoringType>
			<Indicators>
				<IndicatorToMonitor>
					<Code>MULTI_SYMBOL_IND_RNN_LSTM</Code>
				</IndicatorToMonitor>
				
			</Indicators>
		</SecurityToMonitor>
		
	</SecuritiesToMonitor>


	<ChainedTurtleIndicators>
		<ChainedTurtleIndicator>
			<Code>MULTI_SYMBOL_IND_RNN_LSTM</Code>
			<SecuritiesToMonitor>
				<SecurityToMonitor>
					<Symbol>VIX</Symbol>
					<SecurityType>IND</SecurityType>
					<Currency>USD</Currency>
					<Exchange>CBOE</Exchange>
					<MonitoringType>ONLY_ROUTING</MonitoringType>
				</SecurityToMonitor>
				<SecurityToMonitor>
					<Symbol>TLT</Symbol>
					<SecurityType>CS</SecurityType>
					<Currency>USD</Currency>
					<Exchange>SMART</Exchange>
					<MonitoringType>ONLY_ROUTING</MonitoringType>
				</SecurityToMonitor>
				
			</SecuritiesToMonitor>
			
			
		
			<Assembly>tph.ChainedTurtles.BusinessEntities.MonChainedInvExternalSignalIndicator,tph.ChainedTurtles.BusinessEntities</Assembly>
			<SignalType>MULT_SYMBOL_INDICATOR</SignalType>
				<!--BOB=Trendlines,BOB_INV= Trendlines inverted,MULT_SYMBOL_INDICATOR=Multi Symbol Indicator -->
			
		</ChainedTurtleIndicator>

		
	</ChainedTurtleIndicators>
	
	

</ChainedConfiguration>